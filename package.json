{
  "name": "koto",
  "displayName": "Koto",
  "description": "Koto support for Visual Studio Code",
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": ["Programming Languages"],
  "repository": {
    "type": "git",
    "url": "https://github.com/koto-lang/koto-vscode"
  },
  "publisher": "koto-lang",
  "license": "MIT",
  "contributes": {
    "languages": [
      {
        "id": "koto",
        "aliases": ["Koto", "koto"],
        "extensions": ["koto"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "koto",
        "scopeName": "source.koto",
        "path": "./syntaxes/koto.tmLanguage.json"
      }
    ]
  }
}
